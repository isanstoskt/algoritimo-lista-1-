#include <stdio.h>

int main(){
     int quant;
    
    printf("Digite a quantidade de valores que gostaria de adicionar ao vetor\n");
    scanf("%d", &quant);

    double Reais[quant];

    for(int i = 0; i < quant; i++){

        printf("Digite um valor real: \n");
        scanf("%lf", &Reais[i]);

    }

    for(int i = 0; i < quant; i ++){
        
        printf("%.2lf\n", Reais[i]);

    } 

}
