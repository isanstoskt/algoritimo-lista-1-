int maior - NUM1
if (NUM2 > maior){
    maior= NUM2;
}
if (NUM3 > maior) {
    maior= NUM3
}
int menor = NUM1,
if (NUM2 < menor) {
menor = NUM2;
}

    if (NUM1 == NUM2 && NUM2 == NUM3
printf "Os 3 numeros sao iguais);
} else {
